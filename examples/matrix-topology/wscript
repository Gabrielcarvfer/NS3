## -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    obj = bld.create_ns3_program('matrix-topology',
                                 ['network', 'internet', 'netanim', 'point-to-point', 'mobility', 'applications'])
    obj.source = 'matrix-topology.cc'
