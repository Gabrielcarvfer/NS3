lib/dirs.c: Makefile
	($(ro_c) && \
	 echo 'const char ofp_pkgdatadir[] = "$(pkgdatadir$)";' && \
	 echo 'const char ofp_rundir[] = "${localstatedir}/run";' && \
	 echo 'const char ofp_logdir[] = "${localstatedir}/log/${PACKAGE}";') > lib/dirs.c.tmp
	mv lib/dirs.c.tmp lib/dirs.c

